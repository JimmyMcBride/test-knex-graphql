{
  "name": "knex-graphql",
  "version": "1.0.0",
  "description": "An express / knex / graphql database",
  "main": "src/index.js",
  "author": "Jimmy McBride",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node src/index.js",
    "server": "nodemon src/index.js",
    "db-remake": "psql -f remakeDatabase.sql",
    "knex-refresh": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "total-reset": "yarn db-remake && yarn knex-refresh"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^4.0.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^15.0.0",
    "ioredis": "^4.16.2",
    "knex": "^0.20.13",
    "pg": "^8.2.0"
  }
}
